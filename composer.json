{
    "name": "grahamsfault/objectmapper",
    "autoload": {
        "psr-4": {
            "Gb\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": { "Gb\\": "tests/" }
    },
    "require": {
        "sabre/xml": "^1.4",
        "grahamsfault/tadgola": "dev-master",
        "myclabs/php-enum": "^1.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/grahambenevelli/tadgola.git"
        }
    ],
    "scripts": {
        "phpunit": "phpunit --bootstrap vendor/autoload.php tests",
        "now": "phpunit --bootstrap vendor/autoload.php tests --group now"
    }
}
